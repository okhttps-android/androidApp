<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    style="@style/commonPopView"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_common_pop_option_item"
        android:orientation="vertical">

        <TextView
            android:id="@+id/baiduMapTv"
            style="@style/commonPopItemTop"
            android:text="百度地图"
            />

        <View
            android:id="@+id/baiduMapTag"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:background="@color/button_invalid" />

        <TextView
            android:id="@+id/gaodeMapTv"
            style="@style/commonPopItem"
            android:text="高德地图"
            />

        <View
            android:id="@+id/gaodeMapTag"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:background="@color/button_invalid" />

        <TextView
            android:id="@+id/tencentMapTv"
            style="@style/commonPopItem"
            android:text="腾讯地图"
            />

        <View
            android:id="@+id/tencentMapTag"
            android:layout_width="match_parent"
            android:layout_height="0.3dp"
            android:background="@color/button_invalid" />

        <TextView
            android:id="@+id/googleMapTv"
            style="@style/commonPopSingleItem"
            android:text="google地图"
            />

    </LinearLayout>

    <TextView
        android:id="@+id/cancel_tv"
        style="@style/commonPopCancelItem"
        android:text="@string/common_cancel"
        />

</LinearLayout>